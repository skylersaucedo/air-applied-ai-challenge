# Application Settings
PROJECT_NAME=Air AI Integration
VERSION=1.0.0
API_V1_STR=/api/v1

# CORS Settings
CORS_ORIGINS=["http://localhost:3000","http://localhost:8000"]

# AWS Settings
AWS_ACCESS_KEY_ID=your_access_key_id
AWS_SECRET_ACCESS_KEY=your_secret_access_key
AWS_REGION=us-east-2

# S3 Settings
S3_BUCKET_NAME=air-ai-assets-development
S3_ASSET_PREFIX=assets/

# SQS Settings
SQS_QUEUE_URL=https://sqs.us-east-2.amazonaws.com/your-account-id/air-ai-processing-development

# Database Settings
POSTGRES_SERVER=localhost
POSTGRES_USER=air_user
POSTGRES_PASSWORD=air_password
POSTGRES_DB=air_db

# Redis Settings
REDIS_HOST=localhost
REDIS_PORT=6379

# Qdrant Settings
QDRANT_CLUSTER=localhost
QDRANT_PORT=6333

# OpenAI Settings
OPENAI_API_KEY=your_openai_api_key

# Security Settings
SECRET_KEY=your_secret_key_here
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Batch Processing Settings
MAX_BATCH_SIZE=100
BATCH_PROCESSING_TIMEOUT=300

# Monitoring Settings
ENABLE_METRICS=true
ENABLE_TRACING=true 